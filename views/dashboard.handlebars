<!-- Loop over each object in the projects array -->
<h2>Blog Posts</h2>
{{#each userResults as |results|}}
<details>
    
    <summary>
        <a href="/dashboard/{{results.id}}">{{results.title}} - {{format_date results.date_created}}</a>
        {{!-- <span class="icon">ğŸ‘‡</span> --}}
    </summary>
    <p id="post-body">{{results.content}}</p>

</details>
{{/each}}
<button onclick="window.location.href='/blog'" id="submit-btn">New Blog Post</button>
